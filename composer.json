{
    "name": "8fold/php-markup",
    "description": "8fold Markup seeks to make writing semi-structured data easier while allowing for more dynamism.",
    "version": "0.1.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Josh Bruce",
            "email": "josh@8fold.pro"
        }
    ],
    "require": {
        "php": ">=7.0",
        "8fold/php-shoop": "dev-master",
        "nesbot/carbon": "^2.25.0",
        "league/commonmark": "^1.0.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "7.5.6",
        "phpunit/php-code-coverage": "^6.0.7"
    },
    "autoload": {
        "psr-4": {
            "Eightfold\\Markup\\": "./src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Eightfold\\Markup\\Tests\\": "./tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
	"minimum-stability": "dev",
	"prefer-stable": true
}
